<%- include('../../views/partials/admin/header', {title: 'Dashboard'}) %>

        <!-- Main Content -->
        <div class="main-content flex-1 p-6 overflow-auto">
            <!-- Dashboard Content -->
            <div class="bg-white text-black p-6 rounded-lg shadow-lg">
                <div class="mb-6">
                    <h2 class="text-xl font-semibold mb-4">OVER ALL</h2>
                    <div class="grid grid-cols-1 md:grid-cols-4 gap-4">
                        <div class="border border-gray-200 p-4 rounded-lg text-center bg-gray-50">
                            <i class="fas fa-rupee-sign text-gray-500 mb-2"></i>
                            <h3 class="text-sm font-semibold text-gray-600">TOTAL SALE</h3>
                            <p class="text-2xl font-bold text-gray-900">₹9000</p>
                        </div>
                        <div class="border border-gray-200 p-4 rounded-lg text-center bg-gray-50">
                            <i class="fas fa-shopping-cart text-gray-500 mb-2"></i>
                            <h3 class="text-sm font-semibold text-gray-600">TOTAL ORDERS</h3>
                            <p class="text-2xl font-bold text-gray-900">100</p>
                        </div>
                        <div class="border border-gray-200 p-4 rounded-lg text-center bg-gray-50">
                            <i class="fas fa-users text-gray-500 mb-2"></i>
                            <h3 class="text-sm font-semibold text-gray-600">TOTAL CUSTOMER</h3>
                            <p class="text-2xl font-bold text-gray-900">125</p>
                        </div>
                        <div class="border border-gray-200 p-4 rounded-lg text-center bg-gray-50">
                            <i class="fas fa-wallet text-gray-500 mb-2"></i>
                            <h3 class="text-sm font-semibold text-gray-600">TOTAL INCOME</h3>
                            <p class="text-2xl font-bold text-gray-900">₹100K</p>
                        </div>
                    </div>
                </div>

                <!-- Sales Reports -->
                <div class="mb-6">
                    <h2 class="text-xl font-semibold mb-4">SALE REPORTS</h2>
                    <div class="border border-gray-200 p-4 rounded-lg bg-gray-50">
                        <svg class="w-full h-64" viewBox="0 0 600 240" preserveAspectRatio="xMidYMid meet">
                            <!-- Y-Axis Labels -->
                            <text x="20" y="20" fill="black" font-size="12">₹999K</text>
                            <text x="20" y="60" fill="black" font-size="12">₹100K</text>
                            <text x="20" y="100" fill="black" font-size="12">₹50K</text>
                            <text x="20" y="140" fill="black" font-size="12">₹25K</text>
                            <text x="20" y="180" fill="black" font-size="12">₹10K</text>
                            <text x="20" y="220" fill="black" font-size="12">₹1000</text>
                            <!-- Grid Lines -->
                            <line x1="50" y1="10" x2="600" y2="10" stroke="#e5e7eb" stroke-width="1"/>
                            <line x1="50" y1="50" x2="600" y2="50" stroke="#e5e7eb" stroke-width="1"/>
                            <line x1="50" y1="90" x2="600" y2="90" stroke="#e5e7eb" stroke-width="1"/>
                            <line x1="50" y1="130" x2="600" y2="130" stroke="#e5e7eb" stroke-width="1"/>
                            <line x1="50" y1="170" x2="600" y2="170" stroke="#e5e7eb" stroke-width="1"/>
                            <line x1="50" y1="210" x2="600" y2="210" stroke="#e5e7eb" stroke-width="1"/>
                            <!-- X-Axis Labels -->
                            <text x="50" y="230" fill="black" font-size="12">JAN</text>
                            <text x="100" y="230" fill="black" font-size="12">FEB</text>
                            <text x="150" y="230" fill="black" font-size="12">MAR</text>
                            <text x="200" y="230" fill="black" font-size="12">APR</text>
                            <text x="250" y="230" fill="black" font-size="12">MAY</text>
                            <text x="300" y="230" fill="black" font-size="12">JUN</text>
                            <text x="350" y="230" fill="black" font-size="12">JUL</text>
                            <text x="400" y="230" fill="black" font-size="12">AUG</text>
                            <text x="450" y="230" fill="black" font-size="12">SEP</text>
                            <text x="500" y="230" fill="black" font-size="12">OCT</text>
                            <text x="550" y="230" fill="black" font-size="12">NOV</text>
                            <text x="600" y="230" fill="black" font-size="12">DEC</text>
                            <!-- Sales Line -->
                            <polyline points="50,190 100,170 150,130 200,150 250,110 300,90 350,120 400,100 450,80 500,70 550,60 600,50" fill="none" stroke="#FFC107" stroke-width="2"/>
                        </svg>
                    </div>
                </div>

                <!-- Top Selling Products -->
                <div>
                    <h2 class="text-xl font-semibold mb-4">TOP SELLING PRODUCTS</h2>
                    <div class="border border-gray-200 rounded-lg overflow-hidden">
                        <table class="w-full text-left">
                            <thead class="bg-gray-100">
                                <tr class="border-b border-gray-200">
                                    <th class="p-4 font-semibold text-gray-700">PRODUCT</th>
                                    <th class="p-4 font-semibold text-gray-700 text-right">SALES</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr class="border-b border-gray-200 hover:bg-gray-50">
                                    <td class="p-4">CASUAL SHOES WHITE</td>
                                    <td class="p-4 text-right font-medium">101</td>
                                </tr>
                                <tr class="border-b border-gray-200 hover:bg-gray-50">
                                    <td class="p-4">BLACK LEATHER SHOES</td>
                                    <td class="p-4 text-right font-medium">69</td>
                                </tr>
                                <tr class="hover:bg-gray-50">
                                    <td class="p-4">FOOTBALL BOOTS</td>
                                    <td class="p-4 text-right font-medium">44</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

<%- include('../../views/partials/admin/footer') %> 
